{
  "manifest_version": 3,
  "name": "Neural Sherpas",
  "description": "An extension used to detect whether an image is generated by ai or not",
  "version": "1.0",
  "author": "Hero Haru",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "scripting",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search?*"],
      "css": ["index.css"],
      "js": ["extension.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
